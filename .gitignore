# Node.js
#
# This file is a general .gitignore for Node.js projects.
# It aims to cover common files and directories generated by Node.js tools.

# Logs
logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*
lerna-debug.log*

# Runtime data
pids
*.pid
*.seed
*.pid.lock

# Directory for compiled output
dist/
build/

# Dependencies
node_modules/
/jspm_packages/

# Optional npm cache directory
.npm/

# Optional eslint cache
.eslintcache

# Optional REPL history
.node_repl_history

# Optional yarn cache directory
.yarn/cache
.yarn/unplugged

# Optional pnpm store directory
.pnpm-store/

# Environment variables
.env
.env.development.local
.env.test.local
.env.production.local
.env.local

# TypeScript cache
*.tsbuildinfo

# Next.js
.next/

# Nuxt.js
.nuxt/

# Gatsby
.cache/
public/

# VuePress
.vuepress/dist/

# Docusaurus
.docusaurus/
build/

# Serverless
.serverless/

# Storybook
storybook-static/

# VS Code
.vscode/

# macOS
.DS_Store

# Thumbnails
._*

# Editor config
.editorconfig

# Test coverage
coverage/

# IDEs and other tools
.idea/
.project
.classpath
.c9/
.history

# Docker
Dockerfile.bak
docker-compose.yml.bak

# Other
*.tgz
*.swp
*.swo